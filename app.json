{
  "expo": {
    "name": "Mysocial",
    "slug": "Mysocial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fondoWelcome.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fondoWelcome.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anerto.Mysocial"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fondoWelcome.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fondoWelcome.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d27c9666-9886-4fb1-9772-b76c8c556f14"
      }
    }
  }
}
